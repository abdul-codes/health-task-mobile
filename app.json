{
  "expo": {
    "name": "medicTask",
    "slug": "medictask",
    "jsEngine": "hermes",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/medic-icon.png",
    "scheme": "medictask",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aipowered.medicTask"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/medic-icon.png",
        "backgroundColor": "#1F87E0"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aipowered.medicTask",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/medic-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/medic-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1F87E0"
        }
      ],
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "80b0bd59-0b22-4460-bcf4-266f4fafbe35"
      }
    },
    "owner": "abduldev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/80b0bd59-0b22-4460-bcf4-266f4fafbe35"
    }
  }
}
